w Error(\"Required information not found in token\");\n        }\n\n        // localStorage에 저장\n        localStorage.setItem(\"jwtToken\", token);\n        localStorage.setItem(\"memberId\", memberId);\n        localStorage.setItem(\"memberNickname\", memberNickname);\n\n        this.$router.push(\"/\");\n      } catch (error) {\n        console.error(\"Login failed:\", error);\n        alert(\"로그인에 실패했습니다. 이메일과 비밀번호를 확인하세요.\");\n      }\n    }\n  }\n};\n\n  </script>\n  \n  <style scoped>\n\n\nhtml, body {\n  height: 100%;\n  margin: 0;\n  padding: 0;\n  overflow: hidden; /* 스크롤 제거 */\n}\n\n* {\n  box-sizing: border-box;\n}\n\n.container {\n  width: 100vw;\n  height: 100vh;\n  background: url('@/assets/dgu_ground.png') no-repeat center center fixed;\n  background-size: cover;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  position: relative;\n}\n\n.container::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background: rgba(255, 255, 255, 0.5);\n  z-index: 0;\n}\n\n.login-form {\n  position: relative;\n  z-index: 1;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  margin-top: 100px;\n}\n\n.login-content {\n  width: 450px;\n  height: 450px;\n  background: white;\n  padding: 40px;\n  border-radius: 20px;\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);\n  border: 2px solid #3f2702;\n}\n\n.logo-area {\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.input-row {\n  position: relative;\n  margin-bottom: 10px;\n}\n\n.input-row input {\n  width: 100%;\n  height: 48px;\n  padding: 0 15px;\n  border: 1px solid #dadada;\n  border-radius: 6px;\n  font-size: 15px;\n  box-sizing: border-box;\n}\n\n.input-row input:focus {\n  outline: none;\n  border-color: rgba(0, 0, 0, 0.865);\n}\n\n.login-button-area {\n  margin-top: 20px;\n}\n\n.login-button-area button {\n  width: 100%;\n  height: 50px;\n  background-color: #3f2702;\n  color: white;\n  border: none;\n  border-radius: 6px;\n  font-size: 16px;\n  font-weight: 700;\n  cursor: pointer;\n  transition: background-color 0.2s;\n}\n\n.login-button-area button:hover {\n  background-color: #5c3803;\n}\n\n.login-help {\n  margin-top: 20px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-size: 14px;\n}\n\n.right-links {\n  display: flex;\n  align-items: center;\n\n}\n\n.right-links a {\n  color: #3f2702;\n  text-decoration: none;\n  font-size: 13px;\n}\n\n.right-links a:hover {\n  text-decoration: underline;\n  color: #5c3803;\n}\n\n.divider {\n  margin: 0 10px;\n  color: #dadada;\n}\n\n/* 반응형 디자인 */\n@media (max-width: 768px) {\n  .login-form {\n    padding: 20px;\n  }\n\n  .login-content {\n    padding: 20px;\n  }\n\n  .login-help {\n    flex-direction: column;\n    gap: 10px;\n  }\n\n  .right-links {\n    width: 100%;\n    justify-content: center;\n  }\n}\n\n</style>"],"mappings":";OAKeA,UAAwB;;EAJhCC,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAY;;EAChBA,KAAK,EAAC;AAAe;;EAInBA,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAAW;;EAOjBA,KAAK,EAAC;AAAW;;EAOjBA,KAAK,EAAC;AAAmB;;EAGzBA,KAAK,EAAC;AAAY;;EAChBA,KAAK,EAAC;AAAa;;;uBAzBlCC,mBAAA,CAoCM,OApCNC,UAoCM,GAnCJC,mBAAA,CAkCM,OAlCNC,UAkCM,GAjCJD,mBAAA,CAgCM,OAhCNE,UAgCM,G,0BA/BJF,mBAAA,CAEM;IAFDH,KAAK,EAAC;EAAW,IACpBG,mBAAA,CAAoF;IAA/EG,GAAwB,EAAxBP,UAAwB;IAACQ,GAAG,EAAC,KAAK;IAACP,KAAK,EAAC,YAAY;IAACQ,KAAK,EAAC,KAAK;IAACC,MAAM,EAAC;aAEhFN,mBAAA,CA2BM,OA3BNO,UA2BM,GA1BJP,mBAAA,CAMM,OANNQ,UAMM,G,gBALJR,mBAAA,CAIkB;IAJXS,IAAI,EAAC,OAAO;IACZC,EAAE,EAAC,OAAO;IAV7B,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAW4BC,KAAA,CAAAC,SAAS,CAACC,WAAW,GAAAH,MAAA;IAC9BI,WAAW,EAAC,KAAK;IACjBC,QAAQ,EAAR;gCAFSJ,KAAA,CAAAC,SAAS,CAACC,WAAW,E,KAIvCf,mBAAA,CAMM,OANNkB,UAMM,G,gBALJlB,mBAAA,CAIkB;IAJXS,IAAI,EAAC,UAAU;IACfC,EAAE,EAAC,UAAU;IAjBhC,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAkB4BC,KAAA,CAAAC,SAAS,CAACK,cAAc,GAAAP,MAAA;IACjCI,WAAW,EAAC,MAAM;IAClBC,QAAQ,EAAR;gCAFSJ,KAAA,CAAAC,SAAS,CAACK,cAAc,E,KAI1CnB,mBAAA,CAEM,OAFNoB,UAEM,GADJpB,mBAAA,CAAyC;IAAhCqB,OAAK,EAAAV